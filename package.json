{
  "name": "hodwini-web",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "start:prod": "bun run dist/server/index.js",
    "build": "bun run build:server",
    "build:server": "bun build src/index.ts --target bun --outfile dist/server/index.js",
    "build:server:binary": "bun build src/index.ts --compile --target bun --outfile dist/server",
    "preview": "WEB_ROOT=./src/web bun run src/index.ts"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/opentelemetry": "^1.4.1",
    "@elysiajs/static": "^1.4.4",
    "@tanstack/react-query": "^5.90.3",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "drizzle-typebox": "^0.3.3",
    "elysia": "latest"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "^1.3.0"
  },
  "module": "src/index.js"
}
